<div class="container-fluid collection-container">
  <div class="row">

    <div class="col-sm-2" id="cardlist">
      {{card-list session=session cards=model.cards user=model.user cardusers=model.cardusers addCard="addCard"}}
    </div>
    <div class="col-sm-10" id="mycollection">
      <h3 class="section-headers">My Collection:</h3>


      {{cards-you-have session=session join=model.user.card_users}}
      {{my-collection-display session=model.session join=model.user.card_users cardusers=model.cardusers cards=model.cards user=model.user addCard="addCard"}}
    </div>
  </div>
</div>
